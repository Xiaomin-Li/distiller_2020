time python3 compress_classifier.py --arch resnet20_cifar  ../../../data.cifar10 -p=50 --lr=0.3 --epochs=180 --compress=../agp-pruning/myyaml/resnet/resnet20_ft_noslim_30spa.yaml --resume-from=./logs/resnet20_fg_4hy_70spa___2020.05.23-174612/resnet20_fg_4hy_70spa_best.pth.tar --reset-optimizer --vs=0 -n resnet20_hy_30spa_from70fg --gpus 0 &


